!!!!!!!!!!!!!!!!!!!!!!
!! General settings !!
!!!!!!!!!!!!!!!!!!!!!!
#define BORDERPX 2
#define FLOATBORDERPX 0
#define GAPS 23
#define OUTERGAPS 43

!!!!!!!!!!!
!! dmenu !!
!!!!!!!!!!!
dmenu*bottombar: 1

dmenu*x: 660
dmenu*y: 325
dmenu*w: 500
dmenu*h: 30
dmenu*borderpx: 50

dmenu*lines: 10

dmenu*normfgcolor: RAMP1
dmenu*normbgcolor: SECONDARY
dmenu*selfgcolor:  RAMP1
dmenu*selbgcolor:  SECONDARY

dmenu*normhighlightfgcolor: RAMP5
dmenu*normhighlightbgcolor: SECONDARY
dmenu*selhighlightfgcolor:  RAMP5
dmenu*selhighlightbgcolor:  SECONDARY

!dmenu*font: -*-terminal-medium-*-normal--14-140-*-*-*-*-iso8859-*
!dmenu.font: xft:Iosevka:pixelsize=17



!!!!!!!!!!!
!! URxvt !!
!!!!!!!!!!!
Xft.antialias:  true
Xft.rgba:       true
Xft.hinting:    true
Xft.hintstyle:  hintfull

Rxvt*internalBorder: 30
Rxvt*scrollBar: false
Rxvt*letterSpace: 0
Rxvt*iso14755: false
Rxvt*geometry: 85x40

! Change copy-paste keybindds
Rxvt*keysym.Shift-Control-V: eval:paste_clipboard
Rxvt*keysym.Shift-Control-C: eval:selection_to_clipboard
Rxvt*keysym.Control-Meta-c: builtin-string:
Rxvt*keysym.Control-Meta-v: builtin-string:

! Create keybind for scrolling up and down using j and k
Rxvt*keysym.Shift-Control-K: command:\033]720;1\007
Rxvt*keysym.Shift-Control-J: command:\033]721;1\007

Rxvt*perl-ext-common: default,resize-font
Rxvt*url-launcher: /usr/bin/xdg-open
Rxvt*iso14755_52: false
Rxvt*iso8859-1: true

URxvt*background:      #303030
urxvt.depth: 32
urxvt.background: rgba:0000/0000/0000/bbbb
URxvt.tintColor: #262626
URxvt.blurRadius: 5


!!!!!!!!!!!
!! bspwm !!
!!!!!!!!!!!
bspwm.presel_color: YELLOW1
bspwm.gaps:         GAPS
bspwm.outergaps:    OUTERGAPS
bspwm.borderpx:     2

bspwm.outerborderfocused: ACCENT
bspwm.outerbordernormal:  RAMP2
bspwm.innerborderfocused: ACCENT
bspwm.innerbordernormal:  RAMP2

!!!!!!!!!!!!!
!! zathura !!
!!!!!!!!!!!!!
zathura*background: BACKGROUND
zathura*foreground: FOREGROUND
zathura*foreground-alt: BLACK2
zathura*accent: BLUE1
zathura*alert: CYAN1
