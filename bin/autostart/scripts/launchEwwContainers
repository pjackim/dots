#! /bin/bash

while ! eww state; do
    sleep 0.1
done

if pgrep "eww" > /dev/null; then
    eww open m1-left &
    eww open m1-right &
    eww open m1-right-container1 &
    eww open m2-left &
    eww open m2-right &
    eww open m2-right-container1 &
    eww open m3-top-container &
fi
clear
exec "$SHELL"
reset
